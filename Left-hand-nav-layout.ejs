      <!-- Function for Toggling Navbar -->
      <script>
        function openNav() {
          document.getElementById("page-header").style.width = "20%";
          document.getElementById("navDivClosed").style.visibility = "hidden";
          document.getElementById("navDivOpen").style.visibility = "visible";
          document.getElementById("page-footer").style.width = "80%";
          let rightWrapper = document.getElementById("right-wrapper");
          rightWrapper.style.width = "80%";
          document.getElementById("closeNav").style.display = "block";
          document.getElementById("openNav").style.display = "none";
        }
        
        /* Set the width of the side navigation to 0 and the left margin of the page content to 0 */
        function closeNav() {
          document.getElementById("navDivClosed").style.visibility = "visible";
          document.getElementById("page-header").style.width = "5%";
          document.getElementById("navDivOpen").style.visibility = "hidden";
          document.getElementById("page-footer").style.width = "95%";
          let rightWrapper = document.getElementById("right-wrapper");
          rightWrapper.style.width = "95%";
          document.getElementById("closeNav").style.display = "none";
          document.getElementById("openNav").style.display = "block";
        }
      </script>
  
